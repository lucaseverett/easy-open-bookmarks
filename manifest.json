{
  "manifest_version": 3,
  "name": "Easy Open Bookmarks",
  "version": "1.0.0",
  "description": "Click the button to open the Chrome bookmarks page",
  "permissions": ["offscreen"],
  "host_permissions": [],
  "icons": {
    "16": "bookmarks_C7C7C7.png",
    "32": "bookmarks_C7C7C7.png",
    "48": "bookmarks_C7C7C7.png",
    "128": "bookmarks_C7C7C7.png"
  },
  "action": {
    "default_title": "Bookmarks"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "key": ""
}
